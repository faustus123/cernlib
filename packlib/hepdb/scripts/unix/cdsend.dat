#if defined(CERNLIB_UNIX)
#
# Move files between two hosts
#
 
for file in /hepdb/cd*/tovm
 
do
#
# Get prefix
#
 
prefix=`echo $file | awk '{ print substr($file,1,2) "'`
 
if [ prefix == "qq" ]
   then
      continue
fi
done
#endif
