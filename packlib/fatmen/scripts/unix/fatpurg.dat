#
# Purge journal files more than 1 week old
#
#find /fatmen/fm*/done/* -atime +7 -exec rm {} \;
#
# Do it the long way in case there are too many files found
#
for i in /fatmen/fm*/done/*
   do
      find $i -atime +7 -exec rm {} \;
   done
&PATCH,//FATMEN/SCRIPT
&DECK,FMKUIP.
