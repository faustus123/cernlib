SRCS_F = ycompa.F  yedit.F  yfrcet.F  yindex.F  ylist.F  ypatch.F  ysearc.F  yshift.F  ytobcd.F  ytobin.F  ytocet.F

CernlibFortranProgramTarget(ypatchy, ypatch.o, NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(ytobcd,  ytobcd.o, NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(ytobin,  ytobin.o, NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(ytoceta, ytocet.o, NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(yfrceta, yfrcet.o, NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(ycompar, ycompa.o, NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(yedit,   yedit.o , NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(yindexb, yindex.o, NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(ylistb,  ylist.o , NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(ysearch, ysearc.o, NullParameter, NullParameter, p4lib)
CernlibFortranProgramTarget(yshift,  yshift.o, NullParameter, NullParameter, p4lib)

InstallProgram(ypatchy, $(CERN_BINDIR))
InstallProgram(ytobcd, $(CERN_BINDIR))
InstallProgram(ytobin, $(CERN_BINDIR))
InstallProgram(ytoceta, $(CERN_BINDIR))
InstallProgram(yfrceta, $(CERN_BINDIR))
InstallProgram(ycompar, $(CERN_BINDIR))
InstallProgram(yedit, $(CERN_BINDIR))
InstallProgram(yindexb, $(CERN_BINDIR))
InstallProgram(ylistb, $(CERN_BINDIR))
InstallProgram(ysearch, $(CERN_BINDIR))
InstallProgram(yshift, $(CERN_BINDIR))
