jbytet   : proc (msk, a, la, nbits) returns (bin fixed (35));
 
dcl (temp, msk, a, la, nbits) bin fixed (35);
          temp = 0;
          substr (unspec (temp), 37-nbits, nbits) =
substr (unspec (a), 38-la-nbits, nbits);
          unspec(temp) = unspec(temp) & unspec(msk);
          return (temp);
     END jbytet;
