Application comis quit
      subroutine compare(i1,i2,i3)
      vector new,t,nbad
      ibad=0
      do i=i2,i3
         diff=abs(new(i)-t(i))
         if(diff.gt.0.)then
            err=abs(diff/t(i))
         else
            err=0.
         endif
         if(err.gt.2.E-5)then
            if(ibad.eq.0)then
               print *,' ***********SIGMA Test Number',i1,' has failed'
            endif
            ibad=ibad+1
            nbad(1)=ibad
            print*,err, new(i),t(i)
         endif
      enddo
      if(i1.ne.64)return
      print *,' ************************************************'
      if(nbad(1).eq.0)then
         print *,' *     You have a GOOD version of SIGMA         *'
      else
         print *,' *     You have a BAD  version of SIGMA         *'
      endif
      print *,' ************************************************'
      end
quit
